{
    http_port 8080
    order cgi before respond
}

:8080 {
    root * ./maps
    file_server

    # Match everything under /walkthru, not just the exact path
    # Cross-platform: uses MAPSERV_EXECUTABLE environment variable with mapserv default
    cgi /walkthru* {$MAPSERV_EXECUTABLE:mapserv} {
        script_name /walkthru
        dir .
        env MAPSERVER_CONFIG_FILE=./mapserver.conf
        pass_all_env
        windows_header_fix
        # inspect
    }
}